{
    "presets": [
        [
            "@babel/preset-env", 
            {
                "corejs": "3",
                "modules": false,
                "useBuiltIns": "entry",
                "targets": {
                    "browsers": "last 2 versions, not ie <= 8"
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "helpers": false
            }
        ]
    ]
}